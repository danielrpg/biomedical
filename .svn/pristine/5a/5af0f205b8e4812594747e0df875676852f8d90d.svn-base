<div id="tpl_alm_form_egreso_nuevo_ingreso_det" style="display:none;" title="Nuevo Egreso Detalle">
	<h3><img src="../img/alm_egr_32x32.png" align="absmiddle"> Registrar Nuevo Item</h3>
	<hr style="border:1px dashed;">
	<form id="form_alm_nuevo_egreso_det">

  <table border="0">
  
</tr>
  <tr>
     <td><label for="textfield4">PRODUCTO:</label></td>
    <td><input type="text" name="alm_det_desc_egr" id="alm_det_desc_egr" class="txt_campo">
        <input type="hidden" value="" name="cod_unico_producto_egr" id="cod_unico_producto_egr" />
        <input type="hidden" value="" name="resto_ref" id="resto_ref" /><div id="div_error_descp"></div></td>

    
<td><label for="textfield3">CANTIDAD:</label></td>
    <td><input type="text" name="alm_det_cantidad_egr" id="alm_det_cantidad_egr" class="txt_campo" onchange="new Egreso().buscaCantidadEgreso()">
        <input type="text" value="" name="cant_total" id="cant_total" />
        <div id="div_error_cantidad"></div></td>   
  </tr>
  <tr>
    <td>CODIGO INTERNO:</td>
    <td><input type="text" name="alm_det_cod_int_egr" id="alm_det_cod_int_egr" class="txt_campo" readonly="readonly"><div id="div_error_cod_int"></div></td>



    <td><label for="textfield2">REFERENCIA:</label></td>
    <td><input type="text" name="alm_det_ref_egr" id="alm_det_ref_egr" class="txt_campo">
        <input type="hidden" name="alm_det_unico_cab_egr" id="alm_det_unico_cab_egr" class="txt_campo"><div id="div_error_ref"></div></td>


  <tr>

   <td><label for="textfield5">UNIDAD:</label></td>
    <td><input type="text" name="alm_det_piezas_egr" id="alm_det_piezas_egr" class="txt_campo">
        <input type="hidden" name="alm_det_piezas_cod_egr" id="alm_det_piezas_cod_egr" class="txt_campo"><div id="div_error_unidad"></div></td>
    
    <td>&nbsp;</td>
    <td>&nbsp;</td>
  </tr>
</table>
  
        <br>
        <center><input type="submit" value="Registrar Nuevo Egreso" id="bt_submit_alm_nuevo_egreso_det" class="btn_form"><input type="button" value="Cancelar" id="btn_cancelar_egreso_save_Item" class="btn_form" onclick="new Egreso().cancelaItemsEgresoDetalle()"></center>
	</form>
</div>