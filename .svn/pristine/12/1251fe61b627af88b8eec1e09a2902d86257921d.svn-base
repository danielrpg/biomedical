<div id="tpl_vent_form_edit_cliente" style="display:none;" title="Editar Cliente">
	<h3><img src="../img/user office_32x32.png" align="absmiddle"> Editar Datos Cliente </h3>
	<hr style="border:1px dashed;">
	<form id="form_vent_edit_cliente">
		<table>
          <tr>
            <td>Nombre(s):</td>
            <td><input name="txt_vent_nombre_cliente_edit" id="txt_vent_nombre_cliente_edit" type="text" class="txt_campo" /><div id="div_error_vent_error_nombre_edit"></div></td>
            <td>Empresa:</td>
            <td><input name="txt_vent_empresa_cliente_edit" id="txt_vent_empresa_cliente_edit" type="text" class="txt_campo" /></td>
          </tr>
          <tr>
            <td>Apellido Paterno:</td>
            <td><input name="txt_vent_apellido_pat_cliente_edit" id="txt_vent_apellido_pat_cliente_edit" type="text" class="txt_campo"/><div id="div_error_vent_error_apellido_pat_edit"></div></td>
            <td>Cargo:</td>
            <td><input name="txt_vent_cargo_cliente_edit" id="txt_vent_cargo_cliente_edit" type="text" class="txt_campo"/></td>
          </tr>
          <tr>
            <td>Apellido Materno:</td>
            <td><input name="txt_vent_apellido_mat_cliente_edit" id="txt_vent_apellido_mat_cliente_edit" type="text" class="txt_campo"/></td>
            <td>Departamento</td>
            <td><input name="txt_vent_departamento_cliente_edit" id="txt_vent_departamento_cliente_edit" type="text" class="txt_campo"/></td>
          </tr>
          <tr>
            <td>Correo:</td>
            <td><input name="txt_vent_cliente_correo_edit" id="txt_vent_cliente_correo_edit" type="text" class="txt_campo"/><div id="div_error_vent_error_cliente_correo_edit"></div></td>
            <td>Telefono:</td>
            <td><input name="txt_vent_telefono_cliente_edit" id="txt_vent_telefono_cliente_edit" type="text" class="txt_campo"/><div id="div_error_vent_error_telefono_cliente_edit"></div></td>
          </tr>
          <tr>
            <td>Celular:</td>
            <td><input name="txt_vent_celular_cliente_edit" id="txt_vent_celular_cliente_edit" type="text" class="txt_campo"/><div id="div_error_vent_error_celular_cliente_edit"></div></td>
            <td>NIT:</td>
            <td><input name="txt_vent_nit_cliente_edit" id="txt_vent_nit_cliente_edit" type="text" class="txt_campo"/><div id="div_error_vent_error_nit_cliente_edit"></div></td>
          </tr>
          <tr>
            <td>Direcci&oacute;n:</td>
            <td><textarea name="txt_area_vent_dirrecion_cliente_edit" id="txt_area_vent_dirrecion_cliente_edit"></textarea></td>
            <td>Razon Social:</td>
            <td><input name="txt_vent_razon_social_cliente_edit" type="text" id="txt_vent_razon_social_cliente_edit" class="txt_campo"/><div id="div_error_vent_error_razon_social_cliente_edit"></div></td>
          </tr>
          <tr>
            <td>Persona Contactar:</td>
            <td><input name="txt_vent_cliente_persona_contacto_edit" type="text" id="txt_vent_cliente_persona_contacto_edit" class="txt_campo"/><div id="div_error_vent_error_persona_contacto_edit"></div></td>
            <td>N&uacute;mero Interno:</td>
            <td><input name="txt_vent_numero_interno_edit" id="txt_vent_numero_interno_edit" type="text" class="txt_campo"/><div id="div_error_vent_error_numero_interno_edit"></td>
          </tr>
        </table>
        <br>
        <input type="hidden" id="id_unico_cliente" name="id_unico_cliente"><input type="hidden" id="codigo_cliente" name="codigo_cliente" >
        <center><input type="submit" value="Guardar Datos cliente" id="bt_submit_vent_nuevo_cliente" class="btn_form"> <input type="button" value="Cancelar" class="btn_form" onclick="new Cliente().closeDialogEditCliente();"></center>
	</form>
</div>