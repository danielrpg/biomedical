<div id="tpl_alm_form_trasp_nuevo_item_det" style="display:none;" title="Nuevo Traspaso Detalle">
	<h3><img src="../img/traspaso_alm_32x32.png" align="absmiddle"> Registrar Nuevo Item</h3>
	<hr style="border:1px dashed;">
	<form id="form_alm_nuevo_item_trasp_det">

  <table border="0">
    <tr>
      <td><label for="textfield4">Producto:</label></td>
      <td><input type="text" name="alm_det_prod_trasp" id="alm_det_prod_trasp" class="txt_campo">
          <input type="hidden" value="" name="cod_unico_producto_trasp" id="cod_unico_producto_trasp" />
          <!--input type="hidden" value="" name="resto_ref" id="resto_ref" /--><div id="div_error_prod_trasp"></div></td>

      
      <td><label for="textfield3">Cantidad:</label></td>
      <td><input type="text" name="alm_det_cantidad_trasp" id="alm_det_cantidad_trasp" class="txt_campo" onchange="new Index().buscaCantidad()"><div id="div_error_cantidad_trasp"></div></td>   
    </tr>

    <tr>
      <td>Codigo Interno:</td>
      <td><input type="text" name="alm_det_cod_int_trasp" id="alm_det_cod_int_trasp" class="txt_campo" readonly="readonly"><div id="div_error_cod_int_trasp"></div></td>

      <td><label for="textfield2">Referencia:</label></td>
      <td><input type="text" name="alm_det_ref_trasp" id="alm_det_ref_trasp" class="txt_campo">
          <input type="hidden" name="alm_det_unico_cab_trasp" id="alm_det_unico_cab_trasp" class="txt_campo"><div id="div_error_ref_trasp"></div></td>


    </tr>
    <tr>

      <td><label for="textfield5">Unidad:</label></td>
      <td><input type="text" name="alm_det_piezas_trasp" id="alm_det_piezas_trasp" class="txt_campo"><input type="hidden" name="alm_det_piezas_cod_trasp" id="alm_det_piezas_cod_trasp" class="txt_campo"><div id="div_error_unidad_trasp"></div></td>  
      <td>&nbsp;</td>
      <td>&nbsp;</td>
    </tr>
  </table>
        <br>
        <center><input type="submit" value="Registrar Nuevo Item" id="bt_submit_alm_nuevo_traspaso_det" class="btn_form"><input type="button" value="Cancelar" id="btn_cancelar_trasp_save_Item" class="btn_form" onclick="new Traspaso().cancelarItemsTraspDetalle()"></center>
	</form>
</div>